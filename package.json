{
  "name": "pureact",
  "version": "0.4.3",
  "author": "Christian Landgren",
  "license": "MIT",
  "repository": "https://github.com/irony/pureact",
  "main": "dist/index.js",
  "esnext:main": "index.js",
  "dependencies": {
    "vdom": "0.0.22",
    "vtree": "0.0.22"
  },
  "devDependencies": {
    "babel": "5.6.x",
    "babel-preset-es2015": "^6.9.0",
    "mkdirp": "0.5.x",
    "oboy": "^1.2.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha tests/* --compilers js:babel/register",
    "build": "mkdirp dist && ./node_modules/.bin/babel index.js --presets babel-preset-es2015 --out-dir dist/",
    "watch": "./node_modules/.bin/mocha -w tests/* --compilers js:babel/register",
    "prepublish": "npm run build"
  }
}
